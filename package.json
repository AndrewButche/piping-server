    "@types/request": "^2.48.1",
    "@types/yargs": "^12.0.0",
    "espower-typescript": "^9.0.0",
    "get-port": "^4.0.0",
    "mocha": "^5.0.5",
    "power-assert": "^1.4.4",
    "request": "^2.88.0",
